{
	"name": "gitmars",
	"description": "一个高度定制化的git工作流工具",
	"version": "1.3.5-alpha.0",
	"scripts": {
		"eslint": "eslint --fix --ext .vue,.js",
		"prettier": "npx prettier --write bin",
		"lib": "sh build/lib.sh",
		"docs": "sh ./bin/docs.sh --docs",
		"push": "sh ./bin/publish.sh",
		"pub": "sh ./bin/publish.sh --deploy"
	},
	"main": "lib/gitm.js",
	"bin": {
		"gitm": "./lib/gitm.js"
	},
	"dependencies": {
		"colors": "^1.4.0",
		"commander": "6.1.0",
		"core-js": "^3.6.5",
		"execa": "4.0.3",
		"node-apollo": "^1.2.1",
		"ora": "5.1.0",
		"regenerator-runtime": "0.13.7",
		"shelljs": "0.8.4",
		"tracer": "1.1.3"
	},
	"devDependencies": {
		"@babel/cli": "7.11.6",
		"@babel/core": "7.11.6",
		"@babel/preset-env": "7.11.5",
		"eslint": "7.8.1",
		"jsdoc-to-markdown": "^6.0.1",
		"prettier": "2.1.1",
		"uglify-js": "3.10.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/gitmars.git"
	},
	"keywords": [
		"gitmars",
		"git",
		"tool"
	],
