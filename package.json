{
    "name": "gitmars",
    "description": "一个高度定制化的git工作流工具",
    "version": "1.6.0",
    "scripts": {
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:deploy": "GH=1 sh script/deploy-docs.sh",
        "server:start": "node server/bin/www",
        "server:watch": "nodemon server/bin/www",
        "server:super": "supervisor server/bin/www",
        "lib": "sh script/lib.sh",
        "build:docs": "sh script/docs.sh",
        "build:ui": "sh script/build-ui.sh",
        "dist": "run-s eslint prettier lib build:docs build:ui prettier:docs",
        "eslint": "eslint --fix --ext .vue,.js",
        "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,md}\"",
        "prettier:docs": "prettier --write \"API.md\""
    },
    "main": "bin/gitm.js",
    "bin": {
        "gitm": "bin/gitm.js"
    },
    "files": [
        "bin",
        "cache",
        "lib",
        "server"
    ],
    "dependencies": {
        "ci-info": "^3.2.0",
        "colors": "^1.4.0",
        "commander": "8.1.0",
        "cosmiconfig": "^7.0.0",
        "inquirer": "^8.1.2",
        "js-cool": "^2.0.1",
        "node-apollo": "^1.2.1",
        "ora": "6.0.0",
        "shelljs": "^0.8.4",
        "slash": "^3.0.0",
        "text-table": "^0.2.0",
        "tracer": "1.1.5"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.15.0",
        "@microsoft/api-extractor": "^7.18.7",
        "@saqqdy/prettier-config": "^1.0.3",
        "@vssue/api-github-v3": "^1.4.7",
        "@vssue/vuepress-plugin-vssue": "^1.4.7",
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-medium-zoom": "^1.8.2",
        "babel-eslint": "^10.1.0",
        "esbuild": "^0.12.24",
        "eslint": "7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-sets": "^1.3.0",
        "eslint-plugin-jsdoc": "^36.0.7",
        "jsdoc-to-markdown": "^7.0.1",
        "less": "^4.1.1",
        "less-loader": "7.3.0",
        "npm-run-all": "^4.1.5",
        "prettier": "2.3.2",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "supervisor": "^0.12.0",
        "typedoc": "^0.21.9",
        "typedoc-plugin-markdown": "^3.10.4",
        "uglify-js": "3.14.1",
        "vuepress": "^1.8.2",
        "vuepress-plugin-container": "^2.1.4",
        "vuepress-plugin-smooth-scroll": "^0.0.10",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.8.0",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/gitmars.git"
    },
    "keywords": [
        "gitmars",
        "git",
        "tool"
    ],
    "author": "saqqdy <www.saqqdy.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/saqqdy/gitmars/issues"
    },
    "homepage": "https://www.saqqdy.com/gitmars"
}
